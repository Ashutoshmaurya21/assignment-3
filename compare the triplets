#include <bits/stdc++.h>
using namespace std;
int main(){

    int a[3];
    int b[3];
    for(int i=0;i<3;i++){
        cin>>a[i];
    }
    int c=0,d=0;
    for(int i=0;i<3;i++){
        cin>>b[i];
        if(a[i]>b[i])
        c++;
        else if(a[i]<b[i])
        d++;
    }
    cout<<c<<' '<<d;
}
